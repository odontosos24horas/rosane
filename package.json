{
  "name": "odontosos",
  "version": "0.10.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf node_modules .next pnpm-lock.yaml && pnpm store prune",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext .ts,.tsx",
    "precommit": "next lint",
    "commit": "cz",
    "prepush": "git diff HEAD --quiet && git fetch && git pull origin main && pnpm precommit && pnpm i && pnpm build",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/open-sans": "^5.0.17",
    "@fontsource/rajdhani": "^5.0.8",
    "@fontsource/red-hat-display": "^5.0.17",
    "@fontsource/roboto": "^5.0.8",
    "@sendgrid/mail": "^7.7.0",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.5",
    "next": "^14.0.3",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-slick": "^0.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nextime-ui/eslint-config": "^2.6.0",
    "@types/react": "18.2.38",
    "@types/react-slick": "^0.23.12",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "typescript": "^5.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
